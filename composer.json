{
	"name": "serato/sup-sdk-php",
	"homepage": "https://github.com/serato/web-sup-php-sdk",
	"description": "A PHP SDK for sending user profile attributes and events to the Serato User Profile application.",
	"type": "library",
	"require": {
		"php": "^7.1 || ^8.0",
		"aws/aws-sdk-php": "^3.0",
		"ramsey/uuid": "~3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^7 || ^8",
		"squizlabs/php_codesniffer": "^3.0",
		"monolog/monolog": "^1.21.0",
		"phpstan/phpstan": "^1.4",
		"phpstan/phpstan-phpunit": "^1.0",
		"enlightn/security-checker": "^1.4 || ^2"
	},
	"autoload": {
		"psr-4": {
			"Serato\\UserProfileSdk\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\UserProfileSdk\\Test\\": "tests/"
		}
	}
}
